<h2><%= l(:label_working_hours_new) %></h2>

<% labelled_tabular_form_for :working_hours, @working_hours, :url => {:action => 'create', :begindate => params[:begindate], :enddate => params[:enddate], :filter => params[:filter]} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f, :edit => false} %>
  <%= submit_tag l(:button_create) %>
<% end %>

<p><%= link_to l(:button_back), {:action => 'index', :begindate => params[:begindate], :enddate => params[:enddate], :filter => params[:filter]} %></p>
