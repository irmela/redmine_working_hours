<h2><%= l(:label_working_hours_snapshot_edit) %></h2>

<%= labelled_form_for @snapshot, :url => working_hours_snapshot_path(@snapshot) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag l(:button_save) %>
<% end %>

<p><%= link_to l(:button_back), working_hours_snapshots_path %></p>
